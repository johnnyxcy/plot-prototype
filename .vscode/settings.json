{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/__pycache__": true,
    ".venv": true
  },
  "notebook.formatOnSave.enabled": true,
  "jupyter.notebookFileRoot": "${workspaceFolder}",
  "python.envFile": "${workspaceFolder}/.env"
}
